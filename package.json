{
  "name": "react-router-template",
  "version": "1.0.0-0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20",
    "npm": "please use \"pnpm\" to install dependencies üê¢"
  },
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint:inspect": "pnpm eslint --inspect-config",
    "lint": "pnpm lint:es && pnpm lint:style",
    "lint:es": "eslint . --max-warnings=0 --cache",
    "lint:style": "stylelint ./src/**/*.{css,scss} --max-warnings=0 --cache",
    "fix:es": "eslint . --max-warnings=0 --fix",
    "fix:style": "stylelint ./src/**/*.{css,scss} --max-warnings=0 --fix",
    "format:check": "prettier './**/*.{js,jsx,ts,tsx,cjs,mjs,cts,mts,css,scss,md,mdx,yaml,yml,json,html}' --log-level=warn --check --cache",
    "format": "prettier './**/*.{js,jsx,ts,tsx,cjs,mjs,cts,mts,css,scss,md,mdx,yaml,yml,json,html}' --log-level=warn --write",
    "check": "pnpm lint:es && pnpm lint:style && pnpm format:check",
    "check:fix": "pnpm fix:es && pnpm fix:style && pnpm format",
    "test": "vitest --watch",
    "sb": "storybook dev -p 6006",
    "sb:build": "storybook build"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-router/node": "^7.9.6",
    "@react-router/serve": "^7.9.6",
    "@tanstack/react-query": "5.90.11",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.25",
    "isbot": "^5.1.32",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.67.0",
    "react-router": "^7.9.6",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.28.5",
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@react-router/dev": "^7.9.6",
    "@storybook/addon-links": "^10.1.4",
    "@storybook/addon-onboarding": "^10.1.4",
    "@storybook/addon-themes": "^10.1.4",
    "@storybook/react-vite": "^10.1.4",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/eslint-plugin-query": "5.91.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/eslint-plugin": "^1.5.1",
    "autoprefixer": "^10.4.22",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^10.1.4",
    "eslint-plugin-testing-library": "^7.13.5",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "lefthook": "^2.0.7",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "sass-embedded": "^1.93.3",
    "storybook": "^10.1.4",
    "stylelint": "^16.26.1",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-scss": "^6.12.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vite-plugin-babel": "^1.3.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
  }
}
